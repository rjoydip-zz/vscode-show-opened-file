{
    "name": "file-opened",
    "displayName": "File Opened",
    "description": "Show number of file opned in workshop",
    "version": "0.0.1",
    "publisher": "rjoydip",
    "author": {
        "name": "Joydip roy (rjoydip)",
        "email": "joydipand@gmail.com",
        "url": "http://twitter.com/Rjoydip11"
    },
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "file openened"
    ],
    "preview": false,
    "galleryBanner": {
        "color": "#41205f",
        "theme": "dark"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "license": "MIT",
    "icon": "",
    "bugs": {
        "url": "https://github.com/rjoydip/file-opened.git/issues",
        "email": "joydipand@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/rjoydip/file-opened.git"
    },
    "homepage": "https://github.com/rjoydip/file-opened",
    "contributes": {
        "commands": [{
            "command": "extension.fileOpened.go",
            "title": "File opened"
        }]
    },
    "keybindings": [{
        "command": "extension.fileOpened.go",
        "key": "alt+w o",
        "when": "editorTextFocus",
        "mac": "cmd+W O"
    }],
    "menus": {},
    "configuration": {
        "title": "File Opened",
        "properties": {
            "fileOpened.settings.AlertOnLoad": {
                "type": [
                    "boolean"
                ],
                "default": true,
                "description": "Show statusbar alert when editor loads"
            }
        }
    },
    "announcement": {
        "onVersion": "0.0.1",
        "message": "File Opened Update: Mounting a directory is added"
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}